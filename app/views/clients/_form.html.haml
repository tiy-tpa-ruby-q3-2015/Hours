= simple_form_for(@client) do |f|
  = f.error_notification
  .form-inputs
    = f.input :name, required: true, autofocus: true
    = f.input :address
    = f.input :city
    = f.input :state
    = f.input :zip
    = f.input :description, required: false
    = f.input :logo, as: :file
    = f.button :submit
